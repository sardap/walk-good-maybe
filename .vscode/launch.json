{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "game boy launch",
			"type": "clr",
			"request": "launch",
			"program": "mGBA.exe",
			"preLaunchTask": "build-rom",
			"args": [
				"-l",
				"31",
				"${workspaceFolder}\\bin\\walk-good-maybe.gba",
			]
		},
		{
			"name": "music test launch",
			"type": "clr",
			"request": "launch",
			"program": "mGBA.exe",
			"preLaunchTask": "build-rom",
			"args": [
				"${workspaceFolder}\\bin\\music.gba"
			]
		},
		{
			"name": "Launch color agg",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceFolder}/tools/colour-agg",
			"args": [
				"out.png",
				"example.png"
			]
		},
		{
			"name": "Launch builder",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceFolder}/tools/builder",
			"cwd": "${workspaceFolder}",
			"args": [
				"build",
			]
		},
		{
			"name": "Launch cool background gen",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceFolder}/tools/cool-background-gen",
			"cwd": "${workspaceFolder}/tools/cool-background-gen",
			"args": [
				"test.png",
			]
		},
	]
}